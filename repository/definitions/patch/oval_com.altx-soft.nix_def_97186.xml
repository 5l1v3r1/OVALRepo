    <oval-def:title>DSA-4389-1 -- libu2f-host -- security update</oval-def:title>
    <oval-def:affected family="unix">
      <oval-def:platform>Debian 9</oval-def:platform>
      <oval-def:product>libu2f-host</oval-def:product>
